{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "lib": [
            "ES2022",
            "DOM"
        ],
        "moduleResolution": "Bundler",
        "resolveJsonModule": true,
        "strict": true,
        "skipLibCheck": true,
        "verbatimModuleSyntax": true,
        "types": [
            "vite/client"
        ]
    },
    "include": [
        "src/**/*",
        "vite.config.ts"
    ]
}